<style>
.input-text:hover, .textarea:hover {
    border: solid 1px #ddd;
}

body{ padding:50px 0 0 50px;}
.datainp{ width:200px; height:30px; border:1px #ccc solid;}
.datep{ margin-bottom:40px;}
</style>
<link href="lib/webuploader/0.1.5/webuploader.css" rel="stylesheet" type="text/css" />
</head>
<body>

<div class="page-container">
<div id = "errorInfo" style="display: none;"></div>
<form action="#" id ="registerForm" method="post">
	<div action="#" method="post" class="form form-horizontal" id="add">
	
		<div class="row cl">
			<label class="form-label col-xs-2 col-sm-2"><nobr>报修编号：</nobr></label>
			<div class="formControls col-xs-2 col-sm-2" id="repairNumber" style="margin-top:5px;">
				
			</div>
		</div>
	
		<div class="row cl" style="display: none">
			
			<div class="mt-20">
				试模记录
				<table class="table table-border table-bordered table-responsive table-bg">
					<thead>
						<tr class="text-c">
							<th style="width:5%;">序号</th>
							<th>试模开始时间</th>
							<th>试模结束时间</th>
							<th>试模结果</th>
							<th>试模失败原因记入</th>
						</tr>
					</thead>
					<tbody id="mouleList">
					</tbody>
				</table>
		
			</div>
		</div>


		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">报修日期：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" class="input-text" id="reportRepairDate" name="reportRepairDate" style="width:50%" maxlength="20" disabled="disabled">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">报修时间：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" class="input-text" id="reportRepairTime" name="reportRepairTime" style="width:50%" maxlength="20" disabled="disabled">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">报修人：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" class="input-text"  id="applicant" name="applicant" maxlength="20" style="width:50%" disabled="disabled">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">图号：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" class="input-text"  id="figureNumber" name="figureNumber" style="width:50%" maxlength="20" disabled="disabled">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">型名：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" class="input-text"  id="model" name="model" style="width:50%" maxlength="20" disabled="disabled">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">故障时期：</label>
			<div class="formControls col-xs-4 col-sm-4">

				<select name="failurePeriod" id="failurePeriod" class="input-text" style="width:90%" disabled="disabled">
				</select>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">故障内容：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<textarea rows="8" cols="50" style="width:50%" maxlength="50" id="phenomenalDescription" name="phenomenalDescription" disabled="disabled"></textarea>
			</div>
		</div>
		<div class="row cl jeitem" id="jdsj">
			<label class="form-label col-xs-4 col-sm-2">接单开始日期：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="orderTime" name="orderTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
			<label class="form-label col-xs-4 col-sm-2">接单开始时间：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="orderTimeToTime" name="orderTimeToTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
		</div>
		
		<div class="row cl jeitem" id="wxks">
			<label class="form-label col-xs-4 col-sm-2">维修开始日期：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="beginTime" name="beginTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
			<label class="form-label col-xs-4 col-sm-2">维修开始时间：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="beginTimeToTime" name="beginTimeToTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
		</div>
		
		<div class="row cl jeitem" >
			<label class="form-label col-xs-4 col-sm-2">维修结束日期：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="endTime" name="endTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
			<label class="form-label col-xs-4 col-sm-2" style="margin-top:1%;">维修结束时间：</label>
			<div class="formControls col-xs-8 col-sm-9" style="margin-top:1%;">
				<input type="text" id="endTimeToTime" name="endTimeToTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
		</div>
		<div class="row cl" id="scxwxsc">
			<label class="form-label col-xs-4 col-sm-2">用时累计(从报修——维修结束)：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="maintenanceTimeSC" name="maintenanceTimeSC" style="width: 180px" class="input-text" disabled="disabled">
			</div>
		</div>
		<div class="row cl" id="bqxwxsc">
			<label class="form-label col-xs-4 col-sm-2">维修时长(从报修开始——维修结束)：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<input type="text" id="maintenanceTime" name="maintenanceTime" style="width: 180px" class="input-text" disabled="disabled">
			</div>
		</div>
		
	<div style="display:none;">
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">原因：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<textarea rows="8" cols="50" style="width:50%" maxlength="50" id="reason" name="reason" disabled="disabled"></textarea>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">紧急处置：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<textarea rows="8" cols="50" style="width:50%" maxlength="50" id="emergencyDisposal" name="emergencyDisposal" disabled="disabled"></textarea>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-xs-4 col-sm-2">永久对策：</label>
			<div class="formControls col-xs-8 col-sm-9">
				<textarea rows="8" cols="50" style="width:50%" maxlength="50" id="permanentGame" name="permanentGame" disabled="disabled"></textarea>
			</div>
		</div>
		<input type="text" class="input-text" id="id" name="id" style="width:50%;display: none" value="0" />
		<input type="text" class="input-text" id="reId" name="reId" style="width:50%;display: none" value="0" />
		<div class="row cl" id="pictureView">
		</div>
		<div class="row cl" >
			<table class="table table-border table-bordered table-responsive table-bg">
				<tbody id="mouleList">
					<tr>
						<td style="width: 3%">模具担当</td>
		        		<td style="width: 3%" id="mouldUser"></td>
		        		<td style="width: 3%">班长确认</td>
		        		<td style="width: 3%" id="shiftLeader"></td>
		        		<td style="width: 3%">系长确认</td>
		        		<td style="width: 3%" id="preservationDepartment"></td>
		        		<td style="width: 3%" >科长确认</td>
		        		<td style="width: 3%" id="preservationSectionChief"></td>
		        	</tr>
		        	<tr>
						<td style="width: 3%">模具担当</td>
		        		<td style="width: 3%" id=""></td>
		        		<td style="width: 3%">班长确认时间</td>
		        		<td style="width: 3%" id="confirmationTime"></td>
		        		<td style="width: 3%">系长确认确认时间</td>
		        		<td style="width: 3%" id="preDepChiefConTime"></td>
		        		<td style="width: 3%" >科长确认确认时间</td>
		        		<td style="width: 3%" id="preSecChiefConTime"></td>
		        	</tr>
				</tbody>
			</table>

		</div>
	</div>

    </div>
    </form>
</div>


package com.andon.bean;

import org.hibernate.validator.constraints.NotEmpty;

import com.andon.validateInterface.First;

public class Mould extends BaseEntity{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int id;
	
	private String mouldKey;
	
	@NotEmpty(message = "{Mould.vehicleType.empty}", groups = {First.class})
	private String vehicleType;  //车种
	
	@NotEmpty(message = "{Mould.figureNumber.empty}", groups = {First.class})
	private String figureNumber;  //模具品番
	
	@NotEmpty(message = "{Mould.assetCoding.empty}", groups = {First.class})
	private String assetCoding;  //资产编码
	
	@NotEmpty(message = "{Mould.manufacturer.empty}", groups = {First.class})
	private String manufacturer;  //生产厂家
	
	@NotEmpty(message = "{Mould.moldCompletionTime.empty}", groups = {First.class})
	private String moldCompletionTime;  //模具完成时间
	
    private String anotherName; //模具名称

	private int cavityNumber;  //型腔数
	
	@NotEmpty(message = "{Mould.model.empty}", groups = {First.class})
	private String model;  //型号
	
	@NotEmpty(message = "{Mould.useDate.empty}", groups = {First.class})
	private String useDate;  //量产日期
	
	@NotEmpty(message = "{Mould.cuttingDate.empty}", groups = {First.class})
	private String cuttingDate;  //打切日期
	
	@NotEmpty(message = "{Mould.totalWeight.empty}", groups = {First.class})
	private String totalWeight;  //总重量
	
	@NotEmpty(message = "{Mould.outlineDimension.empty}", groups = {First.class})
	private String outlineDimension;  //外形尺寸
	
	@NotEmpty(message = "{Mould.pictureUrl.empty}", groups = {First.class})
	private String pictureUrl;  //3D图   url
	
	@NotEmpty(message = "{Mould.drawingDeposit.empty}", groups = {First.class})
	private Integer drawingDeposit;  //模具类型
	
	private Integer factory;  //工厂
	
	private int annualOutput;  //年产量

	//非数据库字段用于页面显示
	private String factoryName;  //工厂
	
	private String typeName;   //类型中文显示
	
    private String undefined1;
    private String undefined2;
    private String undefined3;
	
	public String getFactoryName() {
		return factoryName;
	}

	public void setFactoryName(String factoryName) {
		this.factoryName = factoryName;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}



	public String getMouldKey() {
		return mouldKey;
	}

	public void setMouldKey(String mouldKey) {
		this.mouldKey = mouldKey;
	}

	public String getVehicleType() {
		return vehicleType;
	}

	public void setVehicleType(String vehicleType) {
		this.vehicleType = vehicleType;
	}

	public String getFigureNumber() {
		return figureNumber;
	}

	public void setFigureNumber(String figureNumber) {
		this.figureNumber = figureNumber;
	}

	public String getAssetCoding() {
		return assetCoding;
	}

	public void setAssetCoding(String assetCoding) {
		this.assetCoding = assetCoding;
	}

	public String getManufacturer() {
		return manufacturer;
	}

	public void setManufacturer(String manufacturer) {
		this.manufacturer = manufacturer;
	}

	public String getMoldCompletionTime() {
		return moldCompletionTime;
	}

	public void setMoldCompletionTime(String moldCompletionTime) {
		this.moldCompletionTime = moldCompletionTime;
	}

	public int getCavityNumber() {
		return cavityNumber;
	}

	public void setCavityNumber(int cavityNumber) {
		this.cavityNumber = cavityNumber;
	}

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public String getUseDate() {
		return useDate;
	}

	public void setUseDate(String useDate) {
		this.useDate = useDate;
	}

	public String getCuttingDate() {
		return cuttingDate;
	}

	public void setCuttingDate(String cuttingDate) {
		this.cuttingDate = cuttingDate;
	}

	public String getTotalWeight() {
		return totalWeight;
	}

	public void setTotalWeight(String totalWeight) {
		this.totalWeight = totalWeight;
	}

	public String getOutlineDimension() {
		return outlineDimension;
	}

	public void setOutlineDimension(String outlineDimension) {
		this.outlineDimension = outlineDimension;
	}

	public String getPictureUrl() {
		return pictureUrl;
	}

	public void setPictureUrl(String pictureUrl) {
		this.pictureUrl = pictureUrl;
	}

	public Integer getDrawingDeposit() {
		return drawingDeposit;
	}

	public void setDrawingDeposit(Integer drawingDeposit) {
		this.drawingDeposit = drawingDeposit;
	}

	public Integer getFactory() {
		return factory;
	}

	public void setFactory(Integer factory) {
		this.factory = factory;
	}

	public int getAnnualOutput() {
		return annualOutput;
	}

	public void setAnnualOutput(int annualOutput) {
		this.annualOutput = annualOutput;
	}

	public String getAnotherName() {
		return anotherName;
	}

	public void setAnotherName(String anotherName) {
		this.anotherName = anotherName;
	}

	public String getUndefined1() {
		return undefined1;
	}

	public void setUndefined1(String undefined1) {
		this.undefined1 = undefined1;
	}

	public String getUndefined2() {
		return undefined2;
	}

	public void setUndefined2(String undefined2) {
		this.undefined2 = undefined2;
	}

	public String getUndefined3() {
		return undefined3;
	}

	public void setUndefined3(String undefined3) {
		this.undefined3 = undefined3;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
	
	
}
